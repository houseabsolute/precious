Description: relax dependency on crate clap
 Needed to match Debian-packaged clap v3.2.16.
Author: Jonas Smedegaard <dr@jones.dk>
Last-Update: 2022-08-19
---
This patch header follows DEP-3: http://dep.debian.net/deps/dep3/
--- a/precious-core/Cargo.toml
+++ b/precious-core/Cargo.toml
@@ -9,7 +9,7 @@
 
 [dependencies]
 anyhow = "1.0.53"
-clap = { version = "3.2.22", features = [ "cargo", "derive" ] }
+clap = { version = "3.2.16", features = [ "cargo", "derive" ] }
 clean-path = "0.2.1"
 fern = { version = ">= 0.5.0, < 0.7.0", features = ["colored"] }
 globset = "0.4.8"
